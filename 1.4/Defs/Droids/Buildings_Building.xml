<?xml version="1.0" encoding="utf-8"?>
<Defs>
  
  <!--COO_Cook_Droid-->
	<ThingDef ParentName="BuildingBase">
		<defName>SWD_COO_Cook_Droid</defName>
		<label>COO Cook Droid</label>
		<description>The COO cook droid is a chef droid created as a joint venture between Industrial Automaton and Publictechnic before the Clone Wars. The droid model comes equipped with over a dozen arms, which are equipped with common cooking tools such as spatulas, knives, and spoons. Designed for catering on a large scale the droids comes with only a few recipes. Cooks one more meal than the standard bulk recipes out of the same amount of ingredients.</description>
		<thingClass>ItemProcessor.Building_ItemProcessor</thingClass>
		<graphicData>
			<texPath>Mechanoid/COO_Cook_Droid</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3.5,3.5)</drawSize>
			<shaderType>CutoutComplex</shaderType>
      <color>(180, 40, 60)</color>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
    <costList>
      <Steel>150</Steel>
      <ComponentIndustrial>5</ComponentIndustrial>
    </costList>
    <altitudeLayer>BuildingOnTop</altitudeLayer>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <Mass>35</Mass>
      <WorkToBuild>10000</WorkToBuild>
      <MaxHitPoints>220</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(3,1)</size>
    <designationCategory>Production</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <constructionSkillPrerequisite>7</constructionSkillPrerequisite>
    <building>
      <isMealSource>true</isMealSource>
      <wantsHopperAdjacent>true</wantsHopperAdjacent>
      <buildingTags>
        <li>Production</li>
      </buildingTags>
    </building>
    <placeWorkers>
      <li>PlaceWorker_PreventInteractionSpotOverlap</li>
    </placeWorkers>
    <researchPrerequisites>
      <li>MicroelectronicsBasics</li>
    </researchPrerequisites>
    <castEdgeShadows>true</castEdgeShadows>
    <staticSunShadowHeight>0.20</staticSunShadowHeight>
    <constructEffect>ConstructMetal</constructEffect>

    <canOverlapZones>false</canOverlapZones>
    <minifiedDef>MinifiedThing</minifiedDef>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>

    <rotatable>true</rotatable>
    <neverMultiSelect>false</neverMultiSelect>
    <uiIconScale>0.8</uiIconScale>
    <tickerType>Rare</tickerType>

    <comps>
			<li Class="CompProperties_HeatPusher">
				<compClass>CompHeatPusherPowered</compClass>
				<heatPerSecond>3</heatPerSecond>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<shortCircuitInRain>false</shortCircuitInRain>
				<basePowerConsumption>400</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable" />
      
			<li Class="ItemProcessor.CompProperties_ItemProcessor">
				<buildingOnGraphic>Mechanoid/COO_Cook_Droid_Working</buildingOnGraphic>
				<numberOfInputs>2</numberOfInputs>
				<acceptsNoneAsInput>false</acceptsNoneAsInput>
				<showFactoryProgressBar>true</showFactoryProgressBar>
				<transfersIngredientLists>false</transfersIngredientLists>
				<isMachineSpecifiesOutput>true</isMachineSpecifiesOutput>
				<isAutoMachine>true</isAutoMachine>
				<isAutoDropper>true</isAutoDropper>
				<isMachinePausable>true</isMachinePausable>
				<isCompletelyAutoMachine>true</isCompletelyAutoMachine>
				<noPowerDestroysProgress>true</noPowerDestroysProgress>
				<rareTicksToDestroy>10</rareTicksToDestroy>
				<destroyIngredientsAtStartOfProcess>true</destroyIngredientsAtStartOfProcess>
				<!--<chooseIngredientsIcon>UI/IP_SetOutput</chooseIngredientsIcon>
				<InsertFirstItemDesc>IP_ChooseOutput</InsertFirstItemDesc>
				<bringIngredientsText>VFEM_BringIngredients</bringIngredientsText>
				<bringIngredientsDesc>VFEM_BringIngredientsDesc</bringIngredientsDesc>
				<bringIngredientsIcon>UI/IP_Begin</bringIngredientsIcon>-->
				<inputSlots>
					<li>(-1,0,1)</li>
					<li>(1,0,1)</li>
				</inputSlots>
			</li>
		</comps>
	</ThingDef>

  <ItemProcessor.CombinationDef>
    <defName>SWD_COO_Cook_Droid_MealSimple</defName>
    <building>SWD_COO_Cook_Droid</building>
    <items>
      <li>FoodRaw</li>
    </items>
    <secondItems>
      <li>FoodRaw</li>
    </secondItems>
    <amount>
      <li>20</li>
      <li>20</li>
    </amount>
    <result>MealSimple</result>
    <yield>5</yield>
    <resultUsesSpecialIcon>true</resultUsesSpecialIcon>
    <resultSpecialIcon>Things/Item/Meal/Simple/Simple_b</resultSpecialIcon>
    <outputLimitControlled>true</outputLimitControlled>
    <maxTotalOutput>100</maxTotalOutput>
    <isCategoryRecipe>true</isCategoryRecipe>
    <useQualityIncreasing>false</useQualityIncreasing>
    <singleTimeIfNotQualityIncreasing>0.019</singleTimeIfNotQualityIncreasing>
  </ItemProcessor.CombinationDef>

